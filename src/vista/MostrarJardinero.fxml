<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controlador.FXMLDocumentController"
            prefWidth="400" prefHeight="250">
    <children>
        <VBox alignment="CENTER_LEFT" spacing="12">
            <padding>
                <Insets top="20" right="20" bottom="20" left="20"/>
            </padding>
            <Label text="Datos del Jardinero"
                   style="-fx-font-size: 16px; -fx-font-weight: bold;"/>
            <Label fx:id="lblNombre"/>
            <Label fx:id="lblApellido"/>
            <Label fx:id="lblGmail"/>
            <Label fx:id="lblSueldo"/>
        </VBox>
    </children>
</AnchorPane>
